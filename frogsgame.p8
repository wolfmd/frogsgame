--keroppi position
k=1
--left foot 0, right foot 1
f=0
--casting 0 no 1 yes
c=0
--bobber pos, hor/vert
h=0
v=0

--bobber bob
b=0

--watermove
w=0
y=0

--fish position
fp=75 + rnd(35)
-- fish sprite/direction 41 is left, 42 is right
fs=41

--fish position
fp2=80 + rnd(30)
-- fish sprite/direction 41 is left, 42 is right
fs2=42

--fish position
fp3=80 + rnd(30)
-- fish sprite/direction 57 is left, 58 is right
fs3=57

function move_water()

	--move fish1
	if (w - flr(w/9)*9==0) then
		if (fs==41) then
			fp=fp-1
		else
			fp=fp+1
		end
	end

	--move fish2
	if (w - flr(w/7)*7==0) then
		if (fs2==41) then
			fp2=fp2-1
		else
			fp2=fp2+1
		end
	end

	--move fish3
	if (w - flr(w/5)*5==0) then
		if (fs3==57) then
			fp3=fp3-1
		else
			fp3=fp3+1
		end
	end


	if (w>75) then
		w=0

		if (y==0) then
			y=1
		else
			y=0
		end
	else
		w=w+1
	end
	if (y==1) then
		mset(6,1,36)
		mset(7,1,37)
		mset(8,1,36)
		mset(9,1,37)
		mset(10,1,38)
		mset(11,1,36)
		mset(12,1,38)
		mset(13,1,36)
		mset(14,1,37)
		mset(15,1,38)
		mset(7,2,37)
		mset(8,2,51)
		mset(9,2,36)
		mset(10,2,36)
		mset(11,2,51)
		mset(12,2,37)
		mset(13,2,38)
		mset(14,2,36)
		mset(15,2,36)
		mset(8,3,51)
		mset(9,3,37)
		mset(10,3,36)
		mset(11,3,37)
		mset(12,3,38)
		mset(13,3,51)
		mset(14,3,36)
		mset(14,3,37)
	else
		mset(6,1,38)
		mset(7,1,36)
		mset(8,1,37)
		mset(9,1,38)
		mset(10,1,36)
		mset(11,1,37)
		mset(12,1,36)
		mset(13,1,37)
		mset(14,1,38)
		mset(15,1,38)
		mset(7,2,38)
		mset(8,2,36)
		mset(9,2,51)
		mset(10,2,38)
		mset(11,2,36)
		mset(12,2,36)
		mset(13,2,37)
		mset(14,2,36)
		mset(15,2,37)
		mset(8,3,36)
		mset(9,3,51)
		mset(10,3,37)
		mset(11,3,38)
		mset(12,3,36)
		mset(13,3,51)
		mset(14,3,37)
		mset(14,3,38)
	end

	--determine fish1 dir
	if (fp<62) then
		fs=42
	elseif (fp>130) then
		fs=41
	end
	spr(fs,fp,100)

	--determine fish2 dir
	if (fp2<67) then
		fs2=42
	elseif (fp2>130) then
		fs2=41
	end
	spr(fs2,fp2,115)

	--determine fish2 dir
	if (fp3<67) then
		fs3=58
	elseif (fp3>140) then
		fs3=57
	end
	spr(fs3,fp3,107)


end

function cast()
	if (b==0) then
		spr(18,k+25+h,67+v)
		line(k+31,43,k+29+h,68+v,6)
	else
		spr(19,k+25+h,67+v)
		line(k+31,43,k+29+h,69+v,6)
	end
	move_water()
end

function draw_keroppi(x)

	--body
	if (c==0) then
		spr(7,x,44)
		spr(8,x+8,44)
		spr(9,x+16,44)
		spr(10,x+8,36)
		spr(23,x,52)
		spr(24,x+8,52)
		spr(25,x+16,52)
	else
		spr(11,x,44)
		spr(12,x+8,44)
		spr(13,x+16,44)
		spr(26,x+23,39)
		spr(27,x,52)
		spr(28,x+8,52)
		spr(29,x+16,52)
		cast()
	end


	--feet
	if (f==0) then
		spr(39,x,60)
		spr(40,x+8,60)
	else
		spr(55,x,60)
		spr(56,x+8,60)
	end
	move_water()
end


