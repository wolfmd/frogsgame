-- frogsgame
-- by mwolf and yvonnedo69

k=1
f=0
c=0
h=0
v=0

function cast()
	spr(18,k+25+h,80+v)
	line(k+31,51,k+29+h,81+v,6)
end

function draw_keroppi(x)

	--body
	if (c==0) then
		spr(7,x,52)
		spr(8,x+8,52)
		spr(9,x+16,52)
		spr(10,x+8,44)
		spr(23,x,60)
		spr(24,x+8,60)
		spr(25,x+16,60)
	else
		spr(11,x,52)
		spr(12,x+8,52)
		spr(13,x+16,52)
		spr(26,x+23,47)
		spr(27,x,60)
		spr(28,x+8,60)
		spr(29,x+16,60)
		cast()
	end


	--feet
	if (f==0) then
		spr(39,x,68)
		spr(40,x+8,68)
	else
		spr(55,x,68)
		spr(56,x+8,68)
	end

end


function _draw()
	cls(12)

	map(0,0,0,72,16,7)

	----water
	--rectfill(54,88,127,127,1)
	--
	--for i=0, 1 do
	--	--land
	--	spr(4,0+24*i,80)
	--	spr(5,8+24*i,80)
	--	spr(6,16+24*i,80)
	--end
	----dock
	--spr(1,48,80)
	--spr(2,56,80)
	--spr(17,64,80)
	--spr(2,72,80)
	--spr(3,80,80)
	--



	print("you are keroppi",
	      34, 2, 7)
	print("press ❎ to cast",
	      32, 10, 7)

	draw_keroppi(k,0)

end

function _update()

	if (k - flr(k/9)*9==0) then
		if(f==0) then
			f=1
		else
			f=0
		end
	end

	--movement, only when not casting

	if (c==0) then
		--move left
		if (btn(0)) then
			if(k==1) then
				draw_keroppi(1)
			else
				k=k-1
				draw_keroppi(k)
			end
		end
		--move right
		if (btn(1)) then
			if(k==80) then
				draw_keroppi(80)
			else
				k=k+1
				draw_keroppi(k)
			end
		end
		--cast
		if k >30 then
			if (btnp(5)) then
				c=1
				if (f==1) then
					f=0
				else
					f=1
				end
				h=flr(rnd(15))
				v=flr(rnd(20))
				draw_keroppi(k)

			end
		end
	else
		--probably see if anything is caught

		--release
		if (btnp(5)) then
			c=0
			draw_keroppi(k)
		end
	end
end
